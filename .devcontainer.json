{
  "build": {
    "args": {},
    "target": "dev",
  },
  "dockerFile": "Dockerfile",
  "extensions": [
    "ms-vscode.go",
    "prisma.vscode-graphql",
    "visualstudioexptteam.vscodeintellicode",
